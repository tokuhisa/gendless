{
  "name": "gendless-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:api-worker && vite",
    "build": "npm run build:api-worker && tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "build:api-worker": "npm run rm:api-worker && npm --prefix \"../gendless-api-worker\" run build && npm install",
    "rm:api-worker": "node -e 'fs.rmSync(`node_modules/gendless-api-worker`, {recursive:true, force:true})'",
    "install:api-worker": "npm install \"../gendless-api-worker\" --link"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "gendless-api-worker": "file:../gendless-api-worker",
    "hastscript": "^9.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-react": "^8.0.0",
    "remark-directive": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwindcss": "^4.1.10",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.7.4",
    "@cloudflare/workers-types": "^4.20250607.0",
    "@eslint/js": "^9.25.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-dom": "^1.51.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-react-x": "^1.51.2",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "rollup-license-plugin": "^3.0.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wrangler": "^4.19.1"
  }
}
